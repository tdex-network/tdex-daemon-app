  "name": "tdex-dashboard",
  "private": true,
  "scripts": {
    "build": "npx craco build",
    "build:proto": "buf generate buf.build/tdex-network/tdex-protobuf && buf generate buf.build/tdex-network/tdex-daemon",
    "bundle-analyzer": "open http://localhost:8888",
    "eject": "react-scripts eject",
    "eslint": "eslint './src/**/*.{ts,tsx}'",
    "fmt": "yarn run eslint --fix && yarn run prettier --write",
    "lint": "yarn run eslint && yarn run prettier --check",
    "prettier": "prettier './src/**/*.{ts,tsx}'",
    "proxy:run": "make run",
    "start": "PORT=3003 nodemon -w './src/assets/styles/antd.customize.less' --exec 'craco start'",
    "start:testnet": "REACT_APP_CHAIN=testnet PORT=3003 nodemon -w './src/assets/styles/antd.customize.less' --exec 'craco start'",
    "tauri:build": "cargo tauri build",
    "tauri:dev": "cargo tauri dev",
    "tdex:clean": "bash scripts/tdex-clean",
    "tdex:connect": "docker exec -it tdexd tdexdconnect --out url",
    "tdex:connect-notls": "docker exec -it tdexd tdexdconnect --out url --no_tls --proto=http",
    "tdex:run": "bash scripts/tdex-run",
    "tdex:run-init": "bash scripts/tdex-run-init",
    "tdex:run-init-notls": "bash scripts/tdex-run-init-notls",
    "tdex:run-insecure": "bash scripts/tdex-run-insecure",
    "tdex:run-notls": "bash scripts/tdex-run-notls",
    "tdex:run-testnet": "bash scripts/tdex-run-testnet",
    "tdexproxy:build-proxy": "docker build --file ./proxy.Dockerfile --tag ghcr.io/tdex-network/tdexd-proxy .",
    "tdexproxy:build-web": "docker build --file ./web.Dockerfile --tag ghcr.io/tdex-network/dashboard .",
    "tdexproxy:clean": "bash scripts/tdex-proxy-clean",
    "tdexproxy:connect": "docker exec -it tdexd-proxy tdexdconnect --out url",
    "tdexproxy:up": "docker-compose up -d",
    "test": "craco test"
  },
  "version": "0.1.56"
}
